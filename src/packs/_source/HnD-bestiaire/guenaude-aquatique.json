    {
      "_id": "x4I5FU3czy81Qif0",
      "name": "Apparence Terrifiante",
      "type": "feat",
      "sort": 200000,
      "flags": {
        "_sheetTab": "description",
        "obsidian": {
          "source": {
            "type": "other"
          },
          "effects": [
            {
              "name": "",
              "uuid": "ab99da61-8e37-459f-ab38-ef6577d07899",
              "components": [
                {
                  "type": "save",
                  "uuid": "d88848a9-9c4a-4295-828a-613756caff40",
                  "calc": "fixed",
                  "fixed": 11,
                  "bonus": 0,
                  "prof": 1,
